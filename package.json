{
  "name": "helpdog",
  "version": "0.1.0",
  "description": "A plugin to embed forms created with MyForm service into WordPress.",
  "author": "noco株式会社",
  "homepage": "https://helpdog.ai/",
  "main": "build/index.js",
  "scripts": {
    "build": "wp-scripts build --webpack-copy-php",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update --dist-tag=wp-6.5",
    "plugin-zip": "wp-scripts plugin-zip",
    "start": "wp-env start && wp-scripts start",
    "start:update": "wp-env start --update && wp-scripts start",
    "stop": "wp-env stop"
  },
  "devDependencies": {
    "@prettier/plugin-php": "^0.22.2",
    "@types/wordpress__block-editor": "^11.5.14",
    "@types/wordpress__blocks": "^12.5.14",
    "@wordpress/env": "^9.3.1",
    "@wordpress/scripts": "^27.2.6",
    "prettier": "^3.2.5"
  }
}
